{
    "name": "athosbenther/laravelapiresponses",
    "description": "This package is intend to help create standardized JSON responses using Laravel | PHP applications.",
    "keywords": [
        "php",
        "laravel",
        "api",
        "responses"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Athos Benther",
            "email": "athos@benther.tech"
        }
    ],
    "require": {
        "php": "^8.3.0",
        "symfony/http-foundation": "^7.1"
    },
    "require-dev": {
        "pestphp/pest": "^3.0.7"
    },
    "autoload": {
        "psr-4": {
            "AthosBenther\\LaravelApiResponses\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}